//КТбо1-5 Водопьянова К.А.
#include <iostream>;
using namespace std;
int main() {
	setlocale(0, "");
	int i, t;
	int n = 12; int s = 0;
	while (n < -1 || n > 5) {
		cout << "Введите число от 0 до 5";
		cin >> n;
	}
	if (n > -1 && n < 6) {
		cout << "Введите" << " " << n << " " << "числа";
	}
	for (int i = 0; i < n; i++) {
		cin >> t;
		s += t;
	}
	cout << s;
}
